{
  "name": "lark-autoloader",
  "version": "2.0.3",
  "description": "A module loader for lark apps",
  "engines": {
    "node": ">=7.6"
  },
  "main": "index.js",
  "scripts": {
    "lint": "./node_modules/.bin/eslint index.js example/**",
    "pretest": "npm run lint && npm link && npm link lark-autoloader",
    "test": "./node_modules/.bin/istanbul cover ./node_modules/.bin/_mocha -- --require should --recursive test",
    "posttest": "npm unlink",
    "debug": "node --inspect-brk=9229 example"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/larkjs/lark-autoloader.git"
  },
  "author": "Sun Haohao",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/larkjs/lark-autoloader/issues"
  },
  "homepage": "https://github.com/larkjs/lark-autoloader#readme",
  "dependencies": {
    "debug": "^2.6.8",
    "directoryfiles": "^2.0.0",
    "vi-misc": "^1.9.2"
  },
  "devDependencies": {
    "eslint": "^3.17.1",
    "eslint-plugin-spellcheck": "0.0.8",
    "istanbul": "^1.1.0-alpha.1",
    "mocha": "^3.2.0",
    "should": "^11.2.1"
  }
}
